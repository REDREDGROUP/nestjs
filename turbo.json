{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "format": {
      "cache": false
    },
    "build:production": {
      "cache": false
    },
    "lint": {},
    "lint:fix": {
      "cache": false
    },
    "type-check": {
      "cache": false
    }
  }
}
